  "keywords": [
    "koa",
    "framework",
    "generator",
    "react-php",
    "middleware"
  ],
  "authors": [
    {
      "name": "York",
      "email": "lianyupeng1988@126.com"
    }
  ],
  "minimum-stability": "stable",
